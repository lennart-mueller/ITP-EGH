<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://primefaces.org/ui"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
 
	
		<managed-bean>
		<managed-bean-name>formMB</managed-bean-name>
		<managed-bean-class>Beans.FormMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		</managed-bean>
	
		<managed-bean>
		<managed-bean-name>testMB</managed-bean-name>
		<managed-bean-class>Beans.Test</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		</managed-bean>
		
		<managed-bean>
		<managed-bean-name>userMB</managed-bean-name>
		<managed-bean-class>Beans.UserMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		</managed-bean>

		<navigation-rule>
		<from-view-id>/pages/Answers.xhtml</from-view-id>
		</navigation-rule>
		
		
	

	
	<navigation-rule>
		<from-view-id>/pages/loginPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>formular</from-outcome>
			<to-view-id>/pages/Answers.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/registerPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/pages/loginPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/loginPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>register</from-outcome>
			<to-view-id>/pages/registerPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/loginPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>fastlane</from-outcome>
			<to-view-id>/pages/individualQuestions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/Answers.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>extra</from-outcome>
			<to-view-id>/pages/individualQuestions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/Answers.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/pages/loginPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/individualQuestions.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>form1</from-outcome>
			<to-view-id>/pages/Answers.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
		
	
		

</faces-config>